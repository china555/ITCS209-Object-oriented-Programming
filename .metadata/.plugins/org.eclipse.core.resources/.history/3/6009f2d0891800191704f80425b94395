package week;

public class EggStop {
	public static void excecute() {
		System.out.println("  ________");
		System.out.println(" /        \\");
		System.out.println("/          \\");
		System.out.println("\\          /");
		System.out.println(" \\________/\n");
		
	}
	public static void main(String[] agrs) {
		excecute();
		excecute();
		System.out.println("+----------+");
		System.out.println("  \n\n  ________");
		System.out.println(" /        \\");
		System.out.println("/          \\");
		System.out.println("|   STOP   |");
		System.out.println("\\          /");
		System.out.println(" \\________/");
		System.out.println("+----------+");
	
	}

}
